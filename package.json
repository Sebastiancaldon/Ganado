{
  "name": "agrotrack-api",
  "version": "1.0.0",
  "description": "API para el sistema de gesti√≥n ganadera AgroTrack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles --forceExit",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "test:prepare": "cross-env NODE_ENV=test sequelize db:migrate"
  },
  "keywords": ["ganaderia", "api", "nodejs", "express", "sequelize"],
  "author": "Equipo AgroTrack",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.3.1",
    "sequelize": "^6.31.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": ["/node_modules/"]
  }
}
